{
  "defaultSeverity": "warn",
  "extends": [
      "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "ordered-imports": [false],
    "object-literal-sort-keys": false,
    "no-console": [false]
  },
  "jsRules": {
    "no-console": [false],
    "object-literal-sort-keys": false
  }
}
